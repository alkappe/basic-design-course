/*
* Draw rectangle, circles...
*/

size(400, 400);
// set color of backgorund
background(255, 20, 73); 
// we change the fill color for circles and rectangles
fill(255, 255, 255);
// set the strokes
stroke(0, 0, 0);
// set the weight
strokeWeight(10);
// draw a rect
rect(200, 200, 150, 150);
// draws a circle
ellipse(200, 200, 150, 150);
// draws a circle
ellipse(20, 20, 200, 200);
